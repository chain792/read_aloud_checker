#==========================================================
# ルートディレクトリの設定
#==========================================================
/.env
/db
.DS_Store

#==========================================================
# api側の設定
#==========================================================
# Ignore bundler config.
api/.bundle

# Ignore all logfiles and tempfiles.
api/log/*
api/tmp/*
!api/log/.keep
!api/tmp/.keep

# Ignore pidfiles, but keep the directory.
api/tmp/pids/*
!api/tmp/pids/
!api/tmp/pids/.keep

# Ignore uploaded files in development.
api/storage/*
!api/storage/.keep
api/.byebug_history

# Ignore master key for decrypting credentials and more.
api/config/master.key

api/.cache
api/.irb_history
api/.viminfo
api/.local
api/.DS_Store
api/.lesshst
api/.bash_history

#==========================================================
# front側の設定
#==========================================================
# Logs
front/logs
front/*.log
front/npm-debug.log*
front/yarn-debug.log*
front/yarn-error.log*
front/pnpm-debug.log*
front/lerna-debug.log*

front/node_modules
front/dist
front/dist-ssr
front/*.local

# Editor directories and files
front/.vscode/*
!front/.vscode/extensions.json
front/.idea
front/.DS_Store
front/*.suo
front/*.ntvs*
front/*.njsproj
front/*.sln
front/*.sw?

front/.ash_history
front/.npm
